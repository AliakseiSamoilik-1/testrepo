    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.0.6",
    "p-limit": "^7.2.0",
    "serverless-http": "^4.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/aws-lambda": "^8.10.150",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "bestzip": "^2.2.1",
    "globals": "^13.24.0",
    "mkdirp": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
